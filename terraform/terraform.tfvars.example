# Development environment values
aws_region          = "ap-south-1"
environment         = "dev"
project_name        = "vittcott"
dynamodb_table_name = "Vittcott_Users"

# Container resources
container_cpu    = 256
container_memory = 512

# Docker images (update after pushing to ECR)
node_backend_image    = "vittcott-node:latest"
python_backend_image  = "vittcott-python:latest"
streamlit_image       = "vittcott-streamlit:latest"

# Secrets (DO NOT COMMIT - use terraform.tfvars locally)
# jwt_secret = "your-jwt-secret-here"
# gemini_api_key = "your-gemini-api-key-here"
